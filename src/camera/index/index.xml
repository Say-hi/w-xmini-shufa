<jwqcapsule capsule="{{ capsule }}">
  <text class="cfff fw7">叠影纠错</text>
</jwqcapsule>
<!--搜索 s-->
<view class="search" style="background:url(https://c.jiangwenqiang.com/lqsy/3.png) ;">
  <view style="height: {{ capsules.bottom + capsules.top / 2 }}px;"></view>
  <view style="height: 80rpx; padding:0 30rpx;" class="pr f24">
    <view class="jwqfont jwqsousuo1 f40"></view>
    <input style="background: #fff;border-radius: 20rpx; padding: 10rpx 10rpx 10rpx 60rpx;" placeholder="请输入要搜索的单字" />
  </view>
</view>
<!--搜索 e-->
<!--屋檐 s-->
<view class="eaves"></view>
<!--屋檐 e-->
<!--分类 -s-->
<scroll-view class="tab-scroll bb"
             scroll-with-animation="1"
             scroll-x="1"
             scroll-into-view="tab{{ tabId - 2 }}">
<view class="tab-inside">
  <button hover-class="none" class="tab-item f25 cfff {{ tabIndex == index ? 'active' : '' }} tr5"
          id="tab{{ index }}"
          catchtap="chooseIndex"
          data-index="{{ index }}"
          wx:key
          wx:for="{{ tabArr }}">
    <image src="https://c.jiangwenqiang.com/lqsy/list1.png"></image>
  </button>
</view>
</scroll-view>
<!--分类 -e-->
<!--二级类别 -s-->
<view class="bgfff secondNav">
  <view catchtap="_secondChoose"
        data-index="{{ index }}"
        wx:for="123123"
        wx:key
        class="snItem tr5 f21 ell1 {{ secondIndex == index ? 'active' : '' }}">田英章硬笔楷书7000</view>
</view>
<!--二级类别 -e-->
        <!--名师列表 -s-->
<view class="teacherList">
  <navigator url="/camera/detail/index?id=123" class="listItem bd1 mt20" wx:for="123445" wx:key="*1">
    <image src="https://c.jiangwenqiang.com/lqsy/list1.png"></image>
    <text>一{{index}}</text>
  </navigator>
</view>
        <!--名师列表 -e-->
<!--错误弹窗-->
