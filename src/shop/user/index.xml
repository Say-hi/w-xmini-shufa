<!--用户操作导航 -s-->
<view class="p20 bgfff mt20">
  <view class="f25 c646464 left-pillar">我的流谦书苑</view>
  <view class="df dfac dfb c323232 shopTab">
    <navigator url="{{ item.u }}" wx:for="{{ userNav }}" wx:key="*53" class="df mt20 dfc dfac f24 stItem">
      <image mode="widthFix" class="tabImg" src="{{ item.i }}"></image>
      <text class="mt10">{{ item.t }}</text>
    </navigator>
  </view>
</view>
<!--用户操作导航 -e-->

<!--操作 -s-->
<navigator url="/user/message/index?type=shopcomment" class="arrow p2030 f25 bgfff bd1 jwqfont jwqpinglun1 mt20">　<text class="c323232">我的评价</text></navigator>
<button open-type="contact">
