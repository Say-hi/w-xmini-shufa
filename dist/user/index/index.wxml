<jwqcapsule capsule="{{ capsule }}"></jwqcapsule>
<!--信息 -s-->
<view class="userInfo">
<view style="height: {{ capsules.bottom + capsules.top / 2 }}px;"></view>
<view class="userTop p2030">
  <image mode="aspectFill" class="br" src="https://c.jiangwenqiang.com/api/logo.jpg"></image>
  <view class="utName">
    <view class="f28 cfff">cm厘米</view>
    <view class="f22 cece696 mt10">终身会员</view>
  </view>
  <view catchtap="_toggleSign" class="f21 cfff jwqfont jwqic_pen flex1 tar">个性签名</view>
</view>
<view class='uiOperation cfff tac'>
  <navigator url="{{ item.url }}" wx:for="{{ uiOp }}" wx:key="*18">
    <view class="f28">{{ item.n }}</view>
    <view class="f22 mt10">{{ item.t }}</view>
  </navigator>
</view>
</view>
<!--信息 -e-->
<!--导航 -s-->
<view class="pr" style="height: 190rpx">
  <view class="pa userTab tac f23 cfff br10">
    <navigator url="{{ item.url }}" wx:for="{{ tabArr }}" wx:key="*19">
      <image mode="widthFix" src="{{ item.i || 'https://c.jiangwenqiang.com/api/logo.jpg' }}"></image>
      <view>{{ item.t }}</view>
    </navigator>
  </view>
</view>
<!--导航 -e-->
<!--操作 -s-->
<navigator url="{{ item.url }}" wx:for="{{ op }}" wx:key="*21" class="arrow p2030 f25 bgfff bd1 jwqfont {{ item.i }}" style="color: {{ item.c }};">　<text class="c323232">{{ item.t }}</text></navigator>
<button open-type="contact">
  <view class="arrow p2030 f25 bgfff jwqfont df jwqqunfengkefujingli" style="color: #31cf2d;">　<text class="c323232">联系客服</text></view>
</button>
<!--操作 -e-->
<navigator url="../login/index" class="f29 fw7 tac br10 bgfff login">退出登陆</navigator>

<!--个性签名 -s-->
<view class="mask masktr1" wx:if="{{ sign }}">
  <view class="mask-in br10 animated zoomIn">
    <view class="tac f30 c323232 fw7 mt20">我的个性签名</view>
    <input class="p20 " placeholder="请输入签名" />
    <view class="df">
      <view catchtap="_toggleSign" class="lh80 flex1 tac t30 bt1">取消</view>
      <view catchtap="_toggleSign" class="lh80 flex1 tac t30 bt1 cff362c">确定</view>
    </view>
  </view>
</view>
<!--个性签名 -e-->

<jwqfoot></jwqfoot>
<!--错误弹窗-->
<toast toast="{{ toast }}"></toast>
