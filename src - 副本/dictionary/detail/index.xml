<jwqcapsule capsule="{{ capsule }}">
  <text class="cfff fw7">详细解释</text>
</jwqcapsule>
<view class="result-top bgfff br5 ce7840e f29">
    <!-- <image style='width: 390rpx; height: 391rpx;margin: 0 auto;' src="{{ info.img_name }}"></image> -->
    <view style='font-size: 200rpx;text-align: center;'>{{options.word}}</view>
    <view class='df dfac info mt20'>
       <view class='flex1'>部　首:<text class='c787878'>　{{wInfo.bushou}}</text></view>
       <view class='flex1'>结　构:<text class='c787878'>　{{wInfo.jiegou}}</text></view>
    </view>
    <view class='df dfac info mt20'>
        <view class='flex1'>笔　序:<text class='c787878'>　{{wInfo.bishun}}</text></view>
        <view class='flex1'>繁体字:<text class='c787878'>　{{ wInfo.fanti || '' }}</text></view>
    </view>
  </view>
<view class='result-wrap'>
  <view class='explain bgfff br5 mt10'>
    <!-- <view class='explain-title f26 c787878 bd2'>字源演变</view> -->
    <view class='f23 c646464 ws' style='padding: 24rpx 42rpx 24rpx 21rpx;letter-spacing: 2rpx; line-height: 35rpx;'>
      <text class='cff362c'>【详细解释】</text>
      {{wInfo.explain[0].content}}
    </view>
  </view>
</view>
<!--错误弹窗-->
<toast toast="{{ toast }}"></toast>
 